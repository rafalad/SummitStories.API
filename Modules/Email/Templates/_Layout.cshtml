<!DOCTYPE html>
<html lang="en">

<head>
    <title></title>
    <style>
        * {
            font-family: 'Open Sans Bold', 'Open Sans Regular', 'Open Sans', sans-serif;
        }

        .outer-table {
            width: 100%;
        }

        .inner-table {
            border: 1px solid #9B9B9B;
            margin: 0 auto;
            width: 500px;
        }

        .logo-cell {
            border-bottom: 1px solid #9B9B9B;
            padding: 20px 10px;
            text-align: left;
        }

        .greeting-cell {
            padding: 10px;
            color: #009BA5;
            font-weight: 700;
            font-size: 14pt;
            text-align: left;
        }

        .app-name-cell {
            padding: 10px;
            color: #009BA5;
            font-weight: 700;
            font-size: 14pt;
            vertical-align: middle;
            text-align: left;
        }

        .footer-cell {
            padding: 20px 10px;
            background-color: #009BA5;
            color: #FFFFFF;
            font-size: 15px;
            text-align: left;
        }

        .content-cell {
            padding: 10px;
            text-align: left;
        }

        table {
            border-spacing: 0;
            border-collapse: collapse;
        }

        td {
            padding: 10px;
            white-space: pre-line;
        }

        .invite-note {
            padding: 10px;
            max-width: 90%;
            height: auto;
            border: 1px solid #009BA5;
            white-space: pre-line;
        }

        .invite-container {
            border-top: none;
            border-bottom: none;
            border-left: solid #9B9B9B 1.0pt;
            border-right: solid #9B9B9B 1.0pt;
            padding: 7.5pt 7.5pt 7.5pt 7.5pt;
        }
    </style>

    @RenderSection("styles", required: false)
</head>

<body>
    <table class="outer-table" aria-describedby="outertable">
        <tbody>
            <tr>
                <td style="text-align: center;">
                    <table class="inner-table" aria-describedby="innertable">
                        <tbody>
                            <tr>
                                <td class="logo-cell">
                                    <img src="data:image/png;base64, "
                                         alt="company logo" />
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <table aria-describedby="innermosttable">
                                        
                                        <tbody>
                                            <tr>
                                                <td class="app-name-cell">
                                                    <img src="data:image/png;base64, "
                                                         alt="information" />
                                                </td>
                                                <td class="app-name-cell">
                                                    SummitStories Blog
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </td>
                            </tr>
                            @RenderSection("body", required: false)
                            <tr>
                                <td class="footer-cell">
                                    <span>&copy; @DateTime.Now.Year, SummitStories</span>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </td>
            </tr>
        </tbody>
    </table>
</body>

</html>
